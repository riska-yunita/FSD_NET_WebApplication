@model IEnumerable<FSD_NET_WebApplication.Models.Accounts>

@{
    ViewData["Title"] = "Index";
}

<h1>Accounts</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Employees)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeNIK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EmployeeNIK">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EmployeeNIK">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EmployeeNIK">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
